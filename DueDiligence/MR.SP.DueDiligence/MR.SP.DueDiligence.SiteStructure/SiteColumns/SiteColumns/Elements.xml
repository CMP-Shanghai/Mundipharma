  <Field
      ID="{CCB2BB32-8DD3-4D30-8F0A-6F09EC886751}"  
      Name="UpcomingEvents"
      Group="Due Diligence Columns"
      Type="Calculated"
      DisplayName="UpcomingEvents"
      Required="FALSE">
    <Formula>=IF(AND([Next Deadline]-Today&lt;=14,[Next Deadline]-Today&gt;=0),"Next Step",IF(AND([Review Date]-Today&lt;=14,[Review Date]-Today&gt;=0),"Review Date",""))
    </Formula>
    <FieldRefs>
      <FieldRef Name="NextDeadline" />
      <FieldRef Name="Today" />
      <FieldRef Name="NextStep" />
    </FieldRefs>
  </Field>

  <Field
      ID="{0CD0DDDB-6853-4962-BF00-3CFDF1BF4FC7}"
      Name="Deviationdays"
      Group="Due Diligence Columns"
      Type="Calculated"
      DisplayName="Deviation (days)"
      Required="FALSE">
    <Formula>=IF(ISBLANK([Actual End]),"",IF(ISBLANK([Planned End]),"",[Actual End]-[Planned End]))
    </Formula>
    <FieldRefs>
      <FieldRef Name="ActualEnd" />
      <FieldRef Name="PlannedEnd" />
    </FieldRefs>
  </Field>

